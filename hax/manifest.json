{
  "name": "Gather hax by alfink",
  "description": "The real ghost mode and disables the ringtone",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["https://gather.town/*",
    "webRequest",
    "webRequestBlocking", "browsingData"],
  
 "content_scripts": [
  {
    "matches": ["https://gather.town/*"],
    "js": ["contentScript.js"],
    "run_at": "document_end"
  }
],
"web_accessible_resources": [
"script.js"
]
}
